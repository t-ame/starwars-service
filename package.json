{
  "name": "nodejs-ts-template",
  "version": "1.0.0",
  "description": "Distinct default NodeJs Typescript template",
  "main": "./src/bin/www/index.ts",
  "private": true,
  "scripts": {
    "prod:start": "NODE_ENV=production node --optimize_for_size --max_old_space_size=920 --gc_interval=100 ./dist/bin/www/index.js",
    "dev:start": "NODE_ENV=development npm-run-all --parallel \"dev:watch\" \"prettier:watch\" \"test:watch\"",
    "dev:watch": "nodemon ./src/bin/www/index.ts",
    "build": "webpack",
    "test": "jest",
    "test:watch": "jest --watchAll --collectCoverage=false",
    "test:vulnerability": "npm audit",
    "prettier:watch": "onchange './src/**/*.ts' -- prettier --write {{changed}}",
    "lint": "eslint './src/**/*.ts' --max-warnings 0",
    "clean": "rm -rf coverage dist"
  },
  "engines": {
    "node": ">=12.10.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/distinctai/nodejs-template-ts.git"
  },
  "keywords": [
    "NodeJs",
    "Template"
  ],
  "author": {
    "name": "Distinct Developers",
    "email": "developers@distinct.ai"
  },
  "license": "LicenseRef-LICENSE",
  "bugs": {
    "url": "https://github.com/distinctai/nodejs-template-ts/issues"
  },
  "homepage": "https://github.com/distinctai/nodejs-template-ts#readme",
  "dependencies": {
    "@distinctai/application-logger": "1.0.0-SNAPSHOT-1618915983",
    "@distinctai/node-error-reporting": "1.0.3-SNAPSHOT-1618442538",
    "@types/cors": "2.8.10",
    "@types/express-serve-static-core": "4.17.1",
    "body-parser": "1.19.0",
    "compression": "1.7.4",
    "cookie-parser": "1.4.4",
    "cors": "2.8.5",
    "dotenv": "8.2.0",
    "errorhandler": "1.5.1",
    "express": "4.17.1",
    "express-validator": "6.4.0",
    "helmet": "3.21.2",
    "install": "0.13.0",
    "log4js": "6.1.0",
    "method-override": "3.0.0",
    "morgan": "1.9.1",
    "mysql": "2.18.1",
    "mysql2": "2.1.0",
    "npm": "6.14.6",
    "typeorm": "0.2.22"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.7",
    "@babel/core": "^7.7.7",
    "@babel/node": "^7.7.7",
    "@babel/polyfill": "^7.7.0",
    "@babel/preset-env": "^7.7.7",
    "@babel/preset-typescript": "^7.7.7",
    "@babel/register": "^7.7.7",
    "@types/body-parser": "1.17.1",
    "@types/compression": "1.0.1",
    "@types/cookie-parser": "1.4.2",
    "@types/errorhandler": "0.0.32",
    "@types/express": "4.17.2",
    "@types/helmet": "0.0.45",
    "@types/jest": "24.0.25",
    "@types/method-override": "0.0.31",
    "@types/morgan": "1.7.37",
    "@types/mysql": "2.15.18",
    "@types/node": "13.1.1",
    "@types/sinon": "7.5.1",
    "@types/sinon-express-mock": "1.3.7",
    "@types/supertest": "2.0.8",
    "@typescript-eslint/eslint-plugin": "^2.13.0",
    "@typescript-eslint/parser": "^2.13.0",
    "eslint": "^6.8.0",
